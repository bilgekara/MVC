@using EntityLayer.Concrete
@model List<Category>
@{
    Layout = null;
    ViewData["Title"] = "Home Page";
}

<table>
    <tr>
        <th>ID</th>
        <th>Kategori Adı</th>
        <th>Durumu</th>
        <th>Detaylar</th>
        <th>Sil</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>@item.CategoryID</tr>
            }
</table>